export EDITOR="vim"
bindkey -v

#PS1='O_o \w\$'
alias bdy='bcloud-gui'
alias etc='gnome-control-center'
alias fm='nautilus'
alias gita='git add'
alias gitm='git commit -m'
alias gitu='git push'
alias gitd='git pull'
alias gitc='git clone'
alias gitf='git fetch'
alias gitb='git branch'
alias gitch='git checkout'
alias gitra='git remote add'
alias gitrm='git remote rm'

alias apg='sudo apt-get dist-upgrade'
alias api='sudo apt-get install'
alias apr='sudo apt-get remove'
alias apu='sudo apt-get update && sudo apt-get upgrade && sudo apt-get autoremove && sudo apt-get autoclean'
alias aps='apt-cache search'
alias vas='sudo vim /etc/apt/sources.list'

setopt AUTO_CD

